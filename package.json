  ],
  "license": "MIT",
  "devDependencies": {
    "lerna": "3.20.2",
    "husky": "4.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:urban-bot/urban-bot.git"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=10.0.0"
  }
